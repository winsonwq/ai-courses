{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
"rootDir": ".",
  "types": [
    "node"
  ]
},
  "include": [
    "src/**/*",
    "scripts/**/*"
  ]
}
